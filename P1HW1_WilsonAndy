num1 = int(input("Enter integer: "))
print("You entered:", (num1))
print(num1,"squard is", num1**2)
print("And",num1,"cubed is", num1**3,"!!")
num2 = int(input("Enter another integer: "))
print(num1,"+",num2,"is", num1 + num2)
print (num1,"*",num2,"is", num1 * num2)
